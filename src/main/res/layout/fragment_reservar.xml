<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.josezc.inp_cm.Inicio_View"
    tools:layout_editor_absoluteY="81dp">

    <!-- TODO: Update blank fragment layout -->

    <Spinner
        android:id="@+id/spinner_especialialidades_reservar"
        android:layout_width="206dp"
        android:layout_height="37dp"
        android:layout_marginBottom="36dp"
        app:layout_constraintBottom_toTopOf="@+id/listView_medicos_esp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="11dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="1dp"
        android:text="Seleccion la Especialidad:"
        app:layout_constraintBottom_toTopOf="@+id/spinner_especialialidades_reservar"
        app:layout_constraintEnd_toEndOf="@+id/spinner_especialialidades_reservar"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/listView_medicos_esp"
        android:layout_width="331dp"
        android:layout_height="0dp"
        android:layout_marginBottom="36dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_especialialidades_reservar" />

</android.support.constraint.ConstraintLayout>
